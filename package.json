{
  "name": "@psychollama/signals",
  "version": "0.0.0",
  "description": "A reactive state management tool",
  "author": "Jesse Gibson <JesseTheGibson@gmail.com>",
  "license": "MIT",
  "private": true,
  "type": "module",
  "types": "./dist/signals.d.ts",
  "exports": {
    ".": {
      "require": "./dist/signals.umd.cjs",
      "import": "./dist/signals.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "signals",
    "transaction",
    "state"
  ],
  "scripts": {
    "prepare": "vite build",
    "test": "./bin/run-tests",
    "test:unit": "vitest",
    "test:lint": "eslint src --ext ts --color",
    "test:fmt": "prettier --check src"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@types/eslint__js": "^8.42.3",
    "eslint": "^8.57.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.0",
    "vite": "^5.0.4",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "signal-polyfill": "^0.1.1"
  }
}
